{
  "require": {
    "craftcms/cms": "^3.0.0",
    "craftcms/contact-form": "^2.2",
    "craftcms/feed-me": "^4.0",
    "craftcms/redactor": "^2.4.0",
    "nystudio107/craft-imageoptimize": "^1.6",
    "nystudio107/craft-seomatic": "^3.2",
    "nystudio107/craft-typogrify": "^1.1",
    "superbig/craft3-templateselect": "^2.0",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
